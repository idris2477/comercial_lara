<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_view_move_form_vendedor" model="ir.ui.view">
        <field name="name">	account.view.move.form.vendedor</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="vendedor_producto"/>
                <field name="vendedor_aux" invisible="1"/>
            </xpath>
        </field>
    </record>
</odoo>

